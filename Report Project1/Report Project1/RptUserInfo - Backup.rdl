      <Query>
        <DataSourceName>ReportingDS</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select U.UserId,U.UserName,Ad.Address,Ad.ZipCode from tblUser U 
	inner join tblAddress Ad on U.UserId = Ad.UserId
	where ZipCode = @ZipCode</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@ZipCode">
            <Value>=Parameters!ZipCode.Value</Value>
          </QueryParameter>
